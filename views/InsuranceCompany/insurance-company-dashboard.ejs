  <script>

let search = "";


$(".searchTableList").on("keyup",function(){
        search = $(".searchTableList").val();
        loadAllHospitals();
    })


    $(window).on('load', function () {
      loadAllHospitals();
      $('#submitHospitalRequestByICButton').click(saveHospitalRequestByIC);
      $('#ins-co-signout').click(logout);
    });

    function logout() {
      $.ajax({
        type: "GET",
        url: "/api/insuranceCompany/logout",
        success: function success(result) {
          window.location.href = '/';
        },
        error: function error(error) {
          console.log(error, 'error');
        }
      })
    }

    function saveHospitalRequestByIC() {
      var data = {
        // Get values of inputs by their IDs
        name: $('#hospital-name').val(),
        contactPerson: $('#contact-person-name').val(),
        email: $('#email-address').val(),
        contactNumber: $('#contact-number').val(),
        address: $('#address').val(),
        district: $('#district').val(),
        pincode: $('#pincode').val(),
        state: $('#state').val(),
        password: `${$('#hospital-name').val()}12345678`
      }
      $.ajax({
        type: "POST",
        url: "/api/hospital/sign-up",
        dataType: 'JSON',
        data: data,
        success: function success(result) {
          window.alert('Hospital request sign-up successful');
          window.location.reload(true);
        },
        error: function error(error) {
          console.log(error, 'error');
          window.alert(`${error.responseJSON.message}`);
        }
      })
    };

    $(document).on('click', '.page-link', function(e) {
        e.preventDefault();
        const page = $(this).data('page');
        loadAllHospitals(page);
    });

    function loadAllHospitals(page) {
      $.ajax({
        type: "GET",
        url: "/api/hospital/getAllHospitalsDetailsForTpa",
        // data: { name: search },
        data: { page: page ,name:search,isVerified:"Approve"},
        success: function success(result) {
          let churan = result.items;
          var tr = '';
          $("#leal-tables-body").html("");
          $('#pagination').html(result.paginationHtml);
          if(churan.length ===0){
            $("#leal-tables-body").html(` <tr>
    <td colspan="7" class="text-center"> 
      No Record
    </td>
  </tr>`)
          }
          churan.forEach(i => {
            $("#leal-tables-body").append(`
                    <tr>
                        <td> ${i.name} </td>
                        <td> ${i.email} </td>
                        <td> ${i.contactNumber} </td>
                        <td> ${i.contactPerson} </td>
                        <td> ${i.address} </td>
                        <td> ${i.state} </td>
                        <td> ${i.district} </td>
                        <td> ${i.pincode} </td>
                        <td class="align-middle white-space-nowrap text-end pe-0 ps-4">
                          <div class="btn-reveal-trigger position-static"><button
                          class="btn btn-sm dropdown-toggle dropdown-caret-none transition-none btn-reveal fs-10"
                          type="button" data-bs-toggle="dropdown" data-boundary="window" aria-haspopup="true"
                          aria-expanded="false" data-bs-reference="parent"><span
                            class="fas fa-ellipsis-h fs-10"></span></button>
                          <div class="dropdown-menu dropdown-menu-end py-2"><a class="dropdown-item" onclick="unExcludeHospital('${i._id}')">Un-Exclude</a>
                          <div class="dropdown-divider"></div><a class="dropdown-item text-danger" onclick="excludeHospital('${i._id}')">Exclude</a>
                        </div>
                      </div>
                    </td>
                    </tr>
                   `)
          });
        },
        error: function error(error) {
          console.log(error, 'error');
        }
      })
    }

    function unExcludeHospital(id) {
      window.alert(`Call admin to unexclude`);
      // $.ajax({
      //   type: "PUT",
      //   url: `/api/insuranceCompany/un-exclude-hospital/${id}`,
      //   success: function success(result) {
      //     window.alert(result.message);
      //   },
      //   error: function error(error) {
      //     window.alert(error.responseJSON.message);
      //     console.log(error, 'error');
      //   }
      // })
    }

    function excludeHospital(id) {
      $.ajax({
        type: "POST",
        url: `/api/insuranceCompany/exclude-hospital/${id}`,
        success: function success(result) {
          window.alert(result.message);
        },
        error: function error(error) {
          window.alert(error.responseJSON.message);
          console.log(error, 'error');
        }
      })
    }

  </script>
