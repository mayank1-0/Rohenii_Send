    <script>
        $(window).on('load', function () {
            $('#aboutUsForm').submit(function (e) {
                e.preventDefault();
                addAboutUs();
            })

            $('#admin-logout').click(logout);
        });

        function logout() {
            $.ajax({
                type: "GET",
                url: "/api/admin/logout",
                success: function success(result) {
                    window.location.href = '/';
                },
                error: function error(error) {
                    ;
                }
            })
        }

        function addAboutUs() {
            var data = {
                // Get values of inputs by their IDs
                heading1: $('#heading1').val(),
                paragraph1: $('#paragraph1').val(),

                heading2: $('#heading2').val(),
                paragraph2: $('#paragraph2').val(),

                heading3: $('#heading3').val(),
                paragraph3: $('#paragraph3').val(),

                heading4: $('#heading4').val(),
                paragraph4: $('#paragraph4').val(),

                heading5: $('#heading5').val(),
                paragraph5: $('#paragraph5').val(),

                heading6: $('#heading6').val(),
                paragraph6: $('#paragraph6').val(),

                heading7: $('#heading7').val(),
                paragraph7: $('#paragraph7').val(),

                heading8: $('#heading8').val(),
                paragraph8: $('#paragraph8').val(),

                heading9: $('#heading9').val(),
                paragraph9: $('#paragraph9').val(),

                heading10: $('#heading10').val(),
                paragraph10: $('#paragraph10').val(),
            }
            $.ajax({
                type: "POST",
                url: "/api/about-us/create-about-us",
                dataType: 'JSON',
                data: data,
                success: function success(result) {
                    window.alert('About Us content added successfully');
                    $('#aboutUsForm')[0].reset();  // Reset form fields
                },
                error: function error(error) {
                    ;
                    window.alert(`${error.responseJSON.message}`);
                }
            })
        }

