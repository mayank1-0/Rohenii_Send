  <script>
    $(window).on('load', function () {
      $('#add_news').click(addNews);
      $('#admin-logout').click(logout);
    });

    function logout() {
      $.ajax({
        type: "GET",
        url: "/api/admin/logout",
        success: function success(result) {
          window.location.href = '/';
        },
        error: function error(error) {
          ;
        }
      })
    }

    function addNews() {
      var data = {
        // Get values of inputs by their IDs
        newsContent: $('#news_content').val(),
      }
      $.ajax({
        type: "POST",
        url: "/api/news/add-latest-news",
        dataType: 'JSON',
        data: data,
        success: function success(result) {
          window.alert('News content added successfully');
          window.location.reload(true);
        },
        error: function error(error) {
          ;
          window.alert(`${error.responseJSON.message}`);
        }
      })
    }

  </script>
