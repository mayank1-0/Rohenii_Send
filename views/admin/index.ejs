          error: function error(error) {
            ;
          }
        })
      }

      function getAllUnVerifiedHospitalsCount() {
        $.ajax({
          type: "GET",
          url: "/api/admin/getAllUnVerifiedHospitalsCount",
          success: function success(result) {
            let hospitalCount = result.count;
            $("#total_unregistered_hospital_count").text(hospitalCount);
            $("#number_of_new_hospital_requests").text(`${hospitalCount} New Hospital`);
          },
          error: function error(error) {
            ;
          }
        })
      }

      function getAllUnVerifiedInsuranceCompaniesCount() {
        $.ajax({
          type: "GET",
          url: "/api/admin/getAllUnVerifiedInsuranceCompaniesCount",
          success: function success(result) {
            let insuranceCompaniesCount = result.count;
