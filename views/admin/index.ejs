    <script>

      $(window).on('load', function () {
        getAllVerifiedHospitalsCount();
        getAllVerifiedInsuranceCompaniesCount();
        getAllVerifiedTPAsCount();
        getAllUnVerifiedHospitalsCount();
        getAllUnVerifiedInsuranceCompaniesCount();
        getAllUnVerifiedTPAsCount();
        $('#admin-logout').click(logout);
      });

      function logout() {
        $.ajax({
          type: "GET",
          url: "/api/admin/logout",
          success: function success(result) {
            window.location.href = '/';
          },
          error: function error(error) {
            window.alert(error.responseJSON.message)
          }
        })
      }

      function getAllVerifiedHospitalsCount() {
        $.ajax({
          type: "GET",
          url: "/api/admin/getAllVerifiedHospitalsCount",
          success: function success(result) {
            let hospitalCount = result.count;
            $("#total_registered_hospital_count").text(hospitalCount);
          },
          error: function error(error) {
            ;
          }
        })
      }

      function getAllVerifiedInsuranceCompaniesCount() {
        $.ajax({
          type: "GET",
          url: "/api/admin/getAllVerifiedInsuranceCompaniesCount",
          success: function success(result) {
            let insuranceCompanyCount = result.count;
